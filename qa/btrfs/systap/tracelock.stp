probe kernel.function("*@kernel/spinlock.c").call
{ 
	name = execname() 
    printf(">>>\texec(%s) %s %s\n", name, probefunc(), symname($addr)) 
}

probe kernel.function("*@kernel/spinlock.c").return
{ 
	name = execname() 
    printf("<<<\texec(%s) %s %s\n", name, probefunc(), symname($addr)) 
}

